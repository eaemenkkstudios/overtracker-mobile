<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="studios.eaemenkk.overtracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.ForgotPasswordActivity"></activity>
        <activity android:name=".view.activity.LoginActivity" />
        <activity android:name=".view.activity.FeedActivity" />
        <activity android:name=".view.activity.ProfileActivity" />
        <activity android:name=".view.activity.SignUpActivity" />
    </application>

</manifest>